<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.Widget.AppCompat.TextView.OnPrimary" parent="Base.Widget.AppCompat.TextView">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="Base.Widget.AppCompat.TextView.OnPrimary.SplashScreenLogo" parent="Base.Widget.AppCompat.TextView.OnPrimary">
        <item name="android:textSize">@dimen/_25sdp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SectionHeader" parent="Base.Widget.AppCompat.TextView.OnPrimary">
        <item name="android:textSize">@dimen/_20sdp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextAppearance.Material3.HeadlineLarge.Bold" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>


    <style name="Base.Widget.Material3.CardView.ProfileClickableCard" parent="Base.Widget.Material3.CardView">
        <item name="cardElevation">0dp</item>
        <item name="cardBackgroundColor">?attr/background</item>
        <item name="cardCornerRadius">0dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
</resources>